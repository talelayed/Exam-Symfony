            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
                <a class="nav-link active" aria-current="page" href="{{ path("etudiants") }}">Students list</a>
                <a class="nav-link active" aria-current="page" href="{{ path("add_etudiants")}}">Add student</a>
